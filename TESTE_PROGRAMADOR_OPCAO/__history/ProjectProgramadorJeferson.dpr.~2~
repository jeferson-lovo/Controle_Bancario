program ProjectProgramadorJeferson;

uses
  Vcl.Forms,
  Umenu in 'Umenu.pas' {Form1},
  Ucliente in 'Ucliente.pas' {Form2},
  Uconta in 'Uconta.pas' {Form3},
  Ubanco in 'Ubanco.pas' {Form4},
  Udebito in 'Udebito.pas' {Form5},
  Ucredito in 'Ucredito.pas' {Form6},
  Urelatorio in 'Urelatorio.pas' {Form7},
  Uconsulta in 'Uconsulta.pas' {Form8},
  Udmdados in 'Udmdados.pas' {DataModule1: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm4, Form4);
  Application.CreateForm(TForm5, Form5);
  Application.CreateForm(TForm6, Form6);
  Application.CreateForm(TForm7, Form7);
  Application.CreateForm(TForm8, Form8);
  Application.CreateForm(TDataModule1, DataModule1);
